#include <stdio.h> 
#include <locale.h> 
 
int main() { 
    setlocale(LC_ALL, ""); // встановлення локалі 
    int n, count = 0; // оголошення змінних n та count 
    printf("Введiть число: "); // виводимо повідомлення для користувача 
    scanf("%d", &n); // зчитування значення n зі стандартного вводу 
    for (int i = 1; i <= n / 2; i++) { // цикл від 1 до n/2 
        if (n % i == n / i) { // перевірка, чи є i рівним дільником числа n 
            count++; // якщо так, то збільшуємо лічильник count 
        } 
    } 
    printf("Кiлькiсть рiвних дiльникiв: %d\n", count); // виводимо значення лічильника count 
    return 0; 
}
